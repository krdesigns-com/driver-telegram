{
    "name": "krbotman/driver-telegram",
    "license": "MIT",
    "description": "Telegram driver for BotMan",
    "keywords": [
        "Bot",
        "BotMan",
        "Telegram"
    ],
    "homepage": "http://github.com/botman/driver-telegram",
    "authors": [
        {
            "name": "Marcel Pociot",
            "email": "m.pociot@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "krbotman/botman": "2.6.0"
    },
    "require-dev": {
        "krbotman/studio-addons": "1.6.0",
        "illuminate/contracts": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0",
        "phpunit/phpunit": "^7.0|^8.0",
        "mockery/mockery": "dev-master",
        "ext-curl": "*"
    },
    "autoload": {
        "psr-4": {
            "BotMan\\Drivers\\Telegram\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "cs": "php-cs-fixer fix"
    },
    "extra": {
        "laravel": {
            "providers": [
                "BotMan\\Drivers\\Telegram\\Providers\\TelegramServiceProvider"
            ]
        }
    }
}
